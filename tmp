          function loadMoreDescription(Prefix, Symbol) {



              var url = "/Scripts/ReadMore.ashx?Prefix=" + Prefix + "&Symbol=" + Symbol;



                $.ajax({

                    type: "POST",

                    url: url,

                    success: function (data) {

                        $("#readmoredescription").css("font-weight", "normal");

                        $("#readmoredescription").html(data);

                    }

                });



        }





