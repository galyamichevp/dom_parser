        document.addEventListener("DOMContentLoaded", function () {

            $("#shareButton").on("touchstart", function (e) {

                    var el = $(this);

                    if (!el.hasClass("open")) {

                    el.find("a").addClass("opening");

                    $("#shareButton").removeClass("open");

                    setTimeout(function () {

                        el.find("a").removeClass("opening");

                        el.addClass("open");

                    }, 500);

               }

            });



            var timer;

            var delay = 300;

            $('#shareButton').hover(function () {

                    $("#shareButton").addClass("open");

                }, function () {

                    $("#shareButton").removeClass("open");

                    clearTimeout(timer);

                });



